{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "narahosting/narahosting-code-block",
	"version": "0.1.0",
	"title": "Narahosting Code Block",
	"category": "widgets",
	"icon": "editor-code",
	"description": "Display code snippets with syntax highlighting in a styled code block",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "narahosting-code-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"language": {
			"type": "string",
			"default": ""
		}
	}
}
